      - name: e
        source:
          location: "{{ file.get_source_location("0.1") }}"
        dest:
          location: "0.8"
        converts:
          - to_w
      - name: "-"
        source:
          location: "{{ file.get_source_location("0.0") }}"
        dest:
          location: "0.9"
        converts:
          - to_n
      - name: up_12
        source:
          location: "{{ file.get_source_location("3.2") }}"
        dest:
          location: "1.0"
        converts:
          - to_n
